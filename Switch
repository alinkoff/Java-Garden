import java.util.Scanner;
public class Switch 
{
 public static void main(String[]args)
 {
	 Scanner scan = new Scanner (System.in);
	 int selection;
	 
	 selection=scan.nextInt();
	 switch (selection)
	 {
	 case 1:
		 System.out.println("You selected tomatoes");
	 break;
	 case 2:
		 System.out.println("You selected watermelon");
	 break;
	 case 3:
		 System.out.println("You selected beets");
	 break;
	 case 4:
		 System.out.println("You selected spinach");
	 break;
	 case 5:
		 System.out.println("You selected carrots");
	 break;
	 case 6:
		 System.out.println("You selected onion");
	 break;
	 case 7:
		 System.out.println("You selected broccoli");
	 break;
	 case 8:
		 System.out.println("You selected giant pumpkin");
	 break;
	 case 9:
		 System.out.println("You selected radish");
	 break;
	 case 10:
		 System.out.println("You selected potatoes");
	 break;
	 case 11:
		 System.out.println("You selected green peas");
	 break;
	 case 12:
		 System.out.println("You selected eggplant");
	 break;
	 case 13:
		 System.out.println("You selected garlic");
	 break;
	 case 14:
		 System.out.println("You selected peppers");
	 break;
	 case 15:
		 System.out.println("You selected sweet corn");
	 break;
	 case 16:
		 System.out.println("You selected sweet potatoes");
	 break;
	 case 17:
		 System.out.println("You selected leeks");
	 break;
	 case 18:
		 System.out.println("You selected cauliflower");
	 break;
	 case 19:
		 System.out.println("You selected beans");
	 break;
	 case 20:
		 System.out.println("You selected okra");
	 break;
	 }
 }
}
